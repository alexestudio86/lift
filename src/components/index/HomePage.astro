---
import liftLogo from '../../assets/lift.png';
---

<main class='light-gray w-100 h-100 display-container'>
	<section class='white display-middle' style='width:80%'>
		<div class="cell cell-middle col s4">
			<img src={liftLogo.src} style="width: 100px;" />
		</div>
			<h1 class='cell cell-middle col s8 center'>
				Lift Project
			</h1>
		</div>
		<div class='row'>
			<div class="col s12 center padding-small">
				<a class="button blue" href="/proposal-01">Proposal 01</a>
				</a>
			</div>
			<div class="col s12 center padding-small">
				<a class="button green disabled" href="/proposal-02">Proposal 02</a>
				</a>
			</div>
		</div>
	</section>
</main>

<style>
	.w-100 {
		width: 100%;
	}
	.h-100 {
		height: 100vh;
	}
</style>